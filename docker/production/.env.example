# Personal AI Assistant - Production Environment Template
# Copy to .env and fill in secure values

# Database Credentials (CHANGE THESE!)
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_secure_neo4j_password_here

# API Security Keys (GENERATE NEW ONES!)
MEMORY_API_SECRET_KEY=your_secure_memory_api_key_here
GATEWAY_API_KEYS=user1:secure_key_1,user2:secure_key_2
WEBUI_SECRET_KEY=your_secure_webui_secret_key_here

# External API Keys
OPENAI_API_KEY=your_openai_api_key_here

# Configuration
OLLAMA_MODEL=llama3.2:1b
OPENAI_MODEL=gpt-4
LOG_LEVEL=INFO
ENABLE_SIGNUP=true

# Domain Configuration
DOMAIN=your-domain.com
ADMIN_EMAIL=your-email@domain.com